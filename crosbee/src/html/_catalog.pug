section.catalog
	.catalog__poster.poster(style="background-image:url(@img/catalog/poster/image-1.png)")
		h1.poster__title Каталог
	.catalog__filters.filters
			form(action="" method="post").filters__form
				ul.filters__list 
					li
						select(name="form[]").form
							option(value="1", selected) Все категории
							option(value="2")  Игровые комплексы размера XL
							option(value="3") Игровые комплексы размера L
							option(value="4") Игровые комплексы размера M
					li
						select(name="form[]").form
							option(value="1", selected) Материал
							option(value="2") Материал 1
							option(value="3") Материал 2
							option(value="4") Материал 3
					li
						select(name="form[]").form
							option(value="1", selected) Назначение
							option(value="2")  Назначение 1
							option(value="3") Назначение 2
							option(value="4") Назначение 3
					li
						select(name="form[]").form
							option(value="1", selected) Сортировать
							option(value="2") По размеру (сначала маленькие)
							option(value="3") По размеру (сначала большие)
							option(value="4") По цене (сначала дешевые)
							option(value="5") По цене (сначала дорогие)
				#search.filters__search.search.active
					form(action="", method="post")
						input#search-input.search__input(type="text", name="search-input" placeholder="Игровой комплекс")
						svg#search-btn(width="20", height="20", viewBox="0 0 20 20", fill="none", xmlns="http://www.w3.org/2000/svg")
							g#Search_icon
								path(id="Vector", d="M8 15C11.866 15 15 11.866 15 8C15 4.13401 11.866 1 8 1C4.13401 1 1 4.13401 1 8C1 11.866 4.13401 15 8 15Z", stroke="#383838", stroke-linecap="round", stroke-linejoin="round")
								path(id="Vector_2", d="M13 13L19 19", stroke="#383838", stroke-linecap="round", stroke-linejoin="round")


