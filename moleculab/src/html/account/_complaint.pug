                .order-block__footer.footer-order
                    .footer-order__info.info-order
                        h3.info-order__title Информация о заказе
                        ul.info-order__list
                            li.info-order__item 
                                .info-order__point Покупатель
                                .info-order__data
                                    p Наталья Карандашова
                                    span +7 (918)325-11-78
                            li.info-order__item 
                                .info-order__point Способ оплаты
                                .info-order__data
                                    p Безналичная оплата
                            li.info-order__item 
                                .info-order__point Дата оформления
                                .info-order__data
                                    p 01.04.2023
                            li.info-order__item 
                                .info-order__point Способ доставки
                                .info-order__data
                                    p Курьерской службой
                            li.info-order__item 
                                .info-order__point Адрес доставки
                                .info-order__data
                                    p Краснодар, ул. Володи Головатого, д. 313, МТРЦ «Галерея Краснодар»
                                    span 10:00 - 22:00

                    .footer-order__details.order-details
                        .cart-bill__body
