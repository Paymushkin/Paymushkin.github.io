section.account
    .account__container 
        - const accountComplaintPage = true;
        include ../_breadcrumbs

        .account__body.body-pa
            aside.body-pa__aside.aside-pa
                .aside-pa__header 
                    .aside-pa__avatar
                        img(src="@img/account/avatar.png", alt="")
                    .aside-pa__info 
                        h3.aside-pa__name Наталья Карандашова
                        .aside-pa__label в клубе
                ul.aside-pa__list
                    li.aside-pa__item
                        a(href="./account-personal.html").aside-pa__link 
                            include ../icons/_profile
                            span Учетные данные

                    li.aside-pa__item
                        a(href="./account-orders.html").aside-pa__link 
                            include ../icons/_order
                            span Мои заказы

                    li.aside-pa__item 
                        a(href="./account-payments.html").aside-pa__link 
                            include ../icons/_payment
                            span Платежные профили

                    li.aside-pa__item 
                        a(href="./account-notifications.html").aside-pa__link 
                            include ../icons/_bell
                            span Настройка уведомлений

                    li.aside-pa__item 
                        a(href="/account-info.html").aside-pa__link 
                            include ../icons/_file
                            span Информация для клиента
                    
                    li.aside-pa__item._active 
                        a(href="/account-complaints.html").aside-pa__link 
                            include ../icons/_complaint
                            span Рекламации
                .aside-pa__footer
                    a(href="./logout")
                        include ../icons/_logout
                        span Выйти
            
            .body-pa__main.order-block
                .order-block__header.header-order.header-complaint
                    .header-order__left._content-active
                        h2.header-order__title Рекламация №1021
                        .header-order__status 
                            span.header-order__icon
                                include ../icons/_complaint-check
                                | Новая
                            span от 15.04.2023

                    .header-order__about
                        span 
                            strong Заказ № 1584068240
                        span 08.04.2023

                .order-block__description.description-order 
                    h3.description-order__title Описание рекламации
                    p.description-order__text Товар в заказе оказался не актуален.
                           
                .order-block__products.products-order

                    ul.products-order__list

                        li.products-order__item.product-order 
                            .product-order__image-ibg
                                img(src="@img/products/item-3.png" alt="Гель-документирующая система ChemiScope", itemprop="image")
                            .product-order__center
                                .product-order__content
                                    .product-order__body
                                        h4.product-order__title(itemprop="brand") Senova BS-3031
                                        p.product-order__text(itemprop="description") Гель-документирующая система ChemiScope Гель-документирующая система ChemiScope Гель-документирующая система ChemiScope
                                    .product-order__count 1шт.
                                .product-order__mob-prices
                                    .product-order__mob-price 234 567 ₽
                                    .product-order__mob-price-old 100 334 ₽
                            .product-order__prices
                                .product-order__price 234 567 ₽
                                .product-order__price-old 100 334 ₽

                        li.products-order__item.product-order 
                            .product-order__image-ibg
                                img(src="@img/products/item-3.png" alt="Гель-документирующая система ChemiScope", itemprop="image")
                            .product-order__center
                                .product-order__content
                                    .product-order__body
                                        h4.product-order__title(itemprop="brand") Senova BS-3031
                                        p.product-order__text(itemprop="description") Гель-документирующая система ChemiScope
                                    .product-order__count 1шт.
                                .product-order__mob-prices
                                    .product-order__mob-price 234 567 ₽
